<h1>Listing family_years</h1>

<table>
  <thead>
    <tr>
      <th>Community</th>
      <th>Family</th>
      <th>School year</th>
      <th>Picture</th>
      <th>Notes</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @family_years.each do |family_year| %>
      <tr>
        <td><%= family_year.community_id %></td>
        <td><%= family_year.family_id %></td>
        <td><%= family_year.school_year %></td>
        <td><%= family_year.picture %></td>
        <td><%= family_year.notes %></td>
        <td><%= link_to 'Show', family_year %></td>
        <td><%= link_to 'Edit', edit_family_year_path(family_year) %></td>
        <td><%= link_to 'Destroy', family_year, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Family year', new_family_year_path %>
