<h1>Listing Enrollments</h1>

<table>
  <thead>
    <tr>
      <th>School Year</th>
      <th>Child</th>
      <th>Child Code</th>
      <th>Sponsor</th>
      <th>Sponsor Code</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @enrollments.each do |enrollment| %>
      <tr>
        <td><%= enrollment.school_year %></td>
        <td><%= enrollment.child_id %></td>
        <td><%= enrollment.child_code %></td>
        <td><%= enrollment.sponsor_id %></td>
        <td><%= enrollment.sponsor_code %></td>
        <td><%= link_to 'Show', enrollment %></td>
        <td><%= link_to 'Edit', edit_enrollment_path(enrollment) %></td>
        <td><%= link_to 'Destroy', enrollment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Enrollment', new_enrollment_path %>
